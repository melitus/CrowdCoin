{
  "name": "crodcoin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "benchmark": "truffle test ./benchmark/*",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "compile": "truffle compile",
    "migrate": "truffle migrate --network development",
    "console": "truffle console --network development",
    "rinkeby": "truffle migrate -f 2 --network rinkeby",
    "ropsten": "truffle migrate -f 2 --network ropsten",
    "coverage": "truffle run coverage",
    "test": "truffle test"
  },
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.2.6",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
